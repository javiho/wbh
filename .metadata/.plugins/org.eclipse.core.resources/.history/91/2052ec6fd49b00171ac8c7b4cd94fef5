package com.wbh.wbh_artifact1;

public class WorldLocation {

	private String name;
	private double x;
	private double y;
	
	public WorldLocation(String name, double x, double y){
		this.name = name;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public double getX() {
		return x;
	}

	public void setX(double x) {
		this.x = x;
	}

	public double getY() {
		return y;
	}

	public void setY(double y) {
		this.y = y;
	}
	
}
